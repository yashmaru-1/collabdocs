  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "dev:all": "concurrently --names \"NEXT,HOCUS\" --prefix-colors \"blue,green\" \"npm run dev\" \"npx ts-node -P tsconfig.server.json server/index.ts\"",
